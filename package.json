    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.17.0",
    "@vercel/blob": "^0.19.0",
    "@vercel/kv": "^1.0.1",
    "axios": "^1.6.5",
    "framer-motion": "^10.17.9",
    "lucide-react": "^0.303.0",
    "next": "14.0.4",
    "openai": "^4.24.1",
    "papaparse": "^5.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
