{
  "modes": {
    "spec-pseudocode": {
      "name": "Specification and Pseudocode",
      "description": "Define requirements and create high-level algorithms",
      "agents": ["researcher", "analyst", "architect"]
    },
    "architect": {
      "name": "Architecture Design",
      "description": "Design system architecture and component structure",
      "agents": ["architect", "reviewer", "documenter"]
    },
    "refinement": {
      "name": "Refinement Phase",
      "description": "Optimize and refine implementation details",
      "agents": ["optimizer", "reviewer", "tester"]
    },
    "completion": {
      "name": "Completion Phase",
      "description": "Finalize implementation and integration",
      "agents": ["coder", "tester", "documenter"]
    },
    "tdd": {
      "name": "Test-Driven Development",
      "description": "Write tests first, then implement to pass",
      "agents": ["tester", "coder", "reviewer"]
    },
    "bdd": {
      "name": "Behavior-Driven Development",
      "description": "Define behavior specifications then implement",
      "agents": ["analyst", "tester", "coder"]
    },
    "ddd": {
      "name": "Domain-Driven Design",
      "description": "Focus on domain modeling and business logic",
      "agents": ["architect", "analyst", "coder"]
    },
    "debug": {
      "name": "Debug Mode",
      "description": "Troubleshoot and fix issues",
      "agents": ["debugger", "analyst", "tester"]
    },
    "review": {
      "name": "Code Review",
      "description": "Analyze code quality and suggest improvements",
      "agents": ["reviewer", "security", "optimizer"]
    },
    "optimize": {
      "name": "Performance Optimization",
      "description": "Improve performance and efficiency",
      "agents": ["optimizer", "benchmarker", "reviewer"]
    },
    "refactor": {
      "name": "Code Refactoring",
      "description": "Restructure code without changing behavior",
      "agents": ["reviewer", "optimizer", "tester"]
    },
    "document": {
      "name": "Documentation",
      "description": "Generate comprehensive documentation",
      "agents": ["documenter", "reviewer", "analyst"]
    },
    "test": {
      "name": "Testing Mode",
      "description": "Create and run comprehensive tests",
      "agents": ["tester", "analyst", "reviewer"]
    },
    "deploy": {
      "name": "Deployment Preparation",
      "description": "Prepare for production deployment",
      "agents": ["devops", "security", "tester"]
    },
    "security": {
      "name": "Security Analysis",
      "description": "Analyze and fix security vulnerabilities",
      "agents": ["security", "reviewer", "tester"]
    },
    "integration": {
      "name": "Integration Testing",
      "description": "Test component integration",
      "agents": ["tester", "architect", "debugger"]
    },
    "benchmark": {
      "name": "Performance Benchmarking",
      "description": "Measure and analyze performance metrics",
      "agents": ["benchmarker", "optimizer", "analyst"]
    }
  },
  "config": {
    "defaultMode": "spec-pseudocode",
    "parallelExecution": true,
    "autoMemory": true,
    "neuralLearning": true,
    "maxAgents": 5
  }
}